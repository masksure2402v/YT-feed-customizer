{
  "manifest_version": 3,
  "name": "youtube feed customizer",
  "version": "1.0",
  "description": "...",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
